{
    "name": "IRTX Synthetic parcels to JSprit connector",
    "description":"The models converts a generated synthetic parcel data set into the input format for the JSprit model",
    "version": "1.0.0",
    "status":"draft",
    "dockerImage":"registry.gitlab.com/inlecom/lead/models/parcels-irtx-2-jsprit",
    "url": "https://github.com/Horizon-LEAD/irtx-parcels-jsprit-connector",
    "modelData":  [
        {
            "name": "Parcels path",
            "variableName": "parcels-path",
            "description": "Path to the input file containing the synthetic parcels",
            "isOutput": false,
            "isOutputDir": false,
            "isOptional": false,
            "inputType": "CLI",
            "chartType": "NONE",
            "modelDataType": {
                "name": "file",
                "dataType": "file",
                "extension": "gpkg"
            }
        },
        {
            "name": "Perimeter path",
            "variableName": "perimeter-path",
            "description": "GeoPackage file describing the perimeter of the study area",
            "isOutput": false,
            "isOutputDir": false,
            "isOptional": false,
            "inputType": "CLI",
            "chartType": "NONE",
            "modelDataType": {
                "name": "file",
                "dataType": "file",
                "extension": "gpkg"
            }
        },
        {
            "name": "Output path",
            "variableName": "output-path",
            "description": "Path to the output directory that will contain the operator information",
            "isOutput": true,
            "isOutputDir": false,
